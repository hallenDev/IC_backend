{
  "dfx": "0.14.3",
  "version": 1,
  "canisters": {
    "user_index": {
      "type": "rust",
      "package": "user_index_canister_impl",
      "candid": "backend/canisters/user_index/api/can.did"
    },
    "local_user_index": {
      "type": "rust",
      "package": "local_user_index_canister_impl",
      "candid": "backend/canisters/local_user_index/api/can.did"
    },
    "post_index": {
      "type": "rust",
      "package": "post_index_canister_impl",
      "candid": "backend/canisters/post_index/api/can.did"
    },
    "local_post_index": {
      "type": "rust",
      "package": "local_post_index_canister_impl",
      "candid": "backend/canisters/local_post_index/api/can.did"
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    },
    "ic": {
      "providers": ["https://ic0.app/"],
      "type": "persistent"
    },
    "ic_test": {
      "providers": ["https://ic0.app/"],
      "type": "persistent"
    },
    "web_test": {
      "providers": ["https://ic0.app/"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env"
}
